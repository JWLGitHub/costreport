CREATE TABLE CST_RPRT_FL
(
     CST_RPRT_FL_ID          INTEGER UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
     CST_RPRT_FL_NM          VARCHAR(100)     NOT NULL,
     CST_RPRT_FL_STTS        VARCHAR(50)      NOT NULL,
     CST_RPRT_FL_ADD_JB      VARCHAR(30)      NOT NULL,
     CST_RPRT_FL_ADD_STP     VARCHAR(30)      NOT NULL,
     CST_RPRT_FL_ADD_PRGRM   VARCHAR(30)      NOT NULL,
     CST_RPRT_FL_ADD_MTHD    VARCHAR(30)      NOT NULL,
     CST_RPRT_FL_ADD_DTTIM   DATETIME         NOT NULL,
     CST_RPRT_FL_UPDT_JB     VARCHAR(30)      NULL,
     CST_RPRT_FL_UPDT_STP    VARCHAR(30)      NULL,
     CST_RPRT_FL_UPDT_PRGRM  VARCHAR(30)      NULL,
     CST_RPRT_FL_UPDT_MTHD   VARCHAR(30)      NULL,
     CST_RPRT_FL_UPDT_DTTIM  DATETIME         NULL
);



CREATE TABLE CST_RPRT_FL_PRCSS
(
     CST_RPRT_FL_PRCSS_ID         INTEGER UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
     CST_RPRT_FL_ID               INTEGER UNSIGNED NOT NULL,
     CST_RPRT_FL_PRCSS            VARCHAR(30)      NOT NULL,
     CST_RPRT_FL_PRCSS_TXT        VARCHAR(200)     NULL,
     CST_RPRT_FL_PRCSS_ADD_JB     VARCHAR(30)      NOT NULL,
     CST_RPRT_FL_PRCSS_ADD_STP    VARCHAR(30)      NOT NULL,
     CST_RPRT_FL_PRCSS_ADD_PRGRM  VARCHAR(30)      NOT NULL,
     CST_RPRT_FL_PRCSS_ADD_MTHD   VARCHAR(30)      NOT NULL,
     CST_RPRT_FL_PRCSS_ADD_DTTM   DATETIME         NOT NULL,
     FOREIGN KEY (CST_RPRT_FL_ID) REFERENCES CST_RPRT_FL(CST_RPRT_FL_ID)
);



CREATE TABLE RDS_FILE
(
     FILE_ID          INTEGER UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
     FILE_DIR_CD      CHAR(1)          NOT NULL,
     FILE_TYPE_CD     CHAR(2)          NOT NULL,
     FILE_DT_TM       DATETIME         NOT NULL,
     FILE_NAME        VARCHAR(54)      NOT NULL,
     FILE_DESC_TXT    VARCHAR(255)     NULL,
     SUBM_ORG_ID      CHAR(10)         NULL,
     ORG_TYP_CD       CHAR(1)          NOT NULL,
     ORG_ID           CHAR(10)         NOT NULL,
     STUS_CTGRY_CD    CHAR(2)          NOT NULL,
     STUS_CD          CHAR(2)          NOT NULL,
     STUS_TS          TIMESTAMP        NOT NULL,
     STUS_PGM         VARCHAR(30)      NOT NULL,
     UPTD_PGM         VARCHAR(30)      NULL,
     UPDT_TS          TIMESTAMP        NULL,
     PROCESS_DT       DATE             NOT NULL,
     RECEIPT_DT       DATE             NOT NULL
);



CREATE TABLE ERR_CTG_REF
(
     ERR_CTGRY_CD     CHAR(2)          NOT NULL PRIMARY KEY,
     DESC_TXT         VARCHAR(255)     NOT NULL
);

INSERT INTO ERR_CTG_REF VALUES ('AE', "Application Error");
INSERT INTO ERR_CTG_REF VALUES ('FE', "File Error");



CREATE TABLE ERR_REF
(
     ERR_CD           CHAR(4)          NOT NULL,
     ERR_CTGRY_CD     CHAR(2)          NOT NULL,
     DESC_TXT         VARCHAR(255)     NOT NULL,
     PRIMARY KEY (ERR_CD, ERR_CTGRY_CD),
     FOREIGN KEY (ERR_CTGRY_CD) REFERENCES ERR_CTG_REF(ERR_CTGRY_CD)
);

